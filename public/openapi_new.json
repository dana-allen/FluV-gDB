    "tags":[
        {
            "name":"sequences",
            "display_name":"Sequences",
            "description":"API endpoints to lookup and retrieve sequences, alignments, and meta-data."

        },
        {
          "name":"alignment",
          "display_name":"Alignments",
          "description":"API endpoints to lookup and retrieve sequences, alignments, and meta-data."

        },
